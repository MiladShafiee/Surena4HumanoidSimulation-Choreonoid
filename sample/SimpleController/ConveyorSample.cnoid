              type: pane
              views: [ "Scene" ]
              current: "Scene"
        - 
          type: pane
          views: [ "Message", "Multi Value Seq", "Multi SE3 Seq", "Body State", "Joint Trajectories", "Multi Affine3 Seq", "Pose Roll" ]
          current: "Message"
